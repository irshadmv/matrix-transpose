#include <stdio.h>
#include <string.h>
int main() {
    int a[20][20],m,n,i,j;
    printf("enter the row and column");
    scanf("%d%d",&m,&n);
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("enter the number");
            scanf("%d",&a[i][j]);
        }
    }
     for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d  ",a[j][i]);
        }
        printf("\n");
    }
    return 0;
}
